<!DOCTYPE html>
<html>
<head>
	<title>New Image</title>
</head>
<body>
	<%= form_for [:admin, @image] do |f| %>
			<%= f.label :name, "Name" %>
			<%= f.text_field :name %>

			<%= f.label :image, "Attach an Image" %>
			<%= f.file_field :image %>

			<%= f.submit "Create Image" %>
	<% end %>
</body>
</html>